	</ItemGroup>
	<ItemGroup>
		<None Remove="Snebur.Comunicacao.csproj.vspscc" />
		<None Remove="Snebur.Comunicacao.Net40.csproj.vspscc" />
		<None Remove="Snebur.Comunicacao.Net50.csproj.vspscc" />
	</ItemGroup>
	<ItemGroup>
	  <ProjectReference Include="..\..\..\Core\Snebur.Net.csproj" />
	</ItemGroup>



</Project>
